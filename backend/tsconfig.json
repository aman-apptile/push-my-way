{
  "compilerOptions": {
    "strict": false,
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "moduleResolution": "bundler",
    "module": "ESNext",
    "target": "ESNext",
    "noEmit": true
  },
  "include": ["src", "api"],
  "exclude": ["node_modules", "dist"]
}
